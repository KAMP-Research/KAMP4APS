<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated KAPSModificationmarks -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="edu.kit.ipd.sdq.kamp4aps.model.modificationmarks.presentation.ModificationmarksModelWizardID"
            name="%_UI_ModificationmarksModelWizard_label"
            class="edu.kit.ipd.sdq.kamp4aps.model.modificationmarks.presentation.ModificationmarksModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ModificationmarksModelFile.gif">
         <description>%_UI_ModificationmarksModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated KAPSModificationmarks -->
      <editor
            id="edu.kit.ipd.sdq.kamp4aps.model.modificationmarks.presentation.ModificationmarksEditorID"
            name="%_UI_ModificationmarksEditor_label"
            icon="icons/full/obj16/ModificationmarksModelFile.gif"
            extensions="modificationmarks"
            class="edu.kit.ipd.sdq.kamp4aps.model.modificationmarks.presentation.ModificationmarksEditor"
            contributorClass="edu.kit.ipd.sdq.kamp4aps.model.modificationmarks.presentation.ModificationmarksActionBarContributor">
      </editor>
   </extension>
   
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="edu.kit.ipd.sdq.kamp4aps.model.modificationmarks.editor.objectContributionkaps1"
            objectClass="edu.kit.ipd.sdq.kamp4aps.model.modificationmarks.KAPSModificationRepository">
         <action
               class="edu.kit.ipd.sdq.kamp4aps.model.modificationmarks.editor.ArchitectureBasedWorkplanDerivationAction"
               icon="../edu.kit.ipd.sdq.kamp/icons/run.png"
               id="edu.kit.ipd.sdq.kamp4aps.model.modificationmarks.editor.actionArchitectureBasedWorkplanDerivation"
               label="Step 5: Calculate activities"
               menubarPath="edit">
         </action>
         <action
               class="edu.kit.ipd.sdq.kamp4aps.model.modificationmarks.editor.ChangePropagationAction"
               icon="../edu.kit.ipd.sdq.kamp/icons/run.png"
               id="edu.kit.ipd.sdq.kamp4aps.model.modificationmarks.editor.actionChangePropagation"
               label="Step 3: Calculate change propagation"
               menubarPath="edit">
         </action>
      </objectContribution>
   </extension>
   

</plugin>
